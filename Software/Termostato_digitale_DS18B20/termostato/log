  jQuery(document).ready(function($) {
  //Initializes in wui.js if AJAX favorites are turned on
		var favorites = [];
		wui.favorites.items.add(favorites);
		if (wui && wui.autocomplete) {
		var a = new wui.autocomplete({
		appendTo: '#wuFormR',
		autosubmit: true,
		data: {
		"ski": 1,
		"features": 1
		},
		includeFavorites: true,
		includePWS: true,
		input: "#wuSearchR",
		minLength: 0,
		showConditions: true,
		showFavoriteStars: true
		});
		}
