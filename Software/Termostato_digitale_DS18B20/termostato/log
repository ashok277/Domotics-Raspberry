<script type="text/javascript">
	(function($){
		$('a[id^=aviation_tab_]','#aviation_tabs').click(function(){
		clicked_id = $(this).attr("id");
		clicked_id_array = $(this).attr("id").split("_");
		clickedname = clicked_id_array[2];
		$('a[id^=aviation_tab_]','#aviation_tabs').each(function(){
		if(clicked_id==$(this).attr("id")){
		$(this).removeClass("cmTabActive").removeClass("cmTab").addClass("cmTabActive");
		} else {
		$(this).removeClass("cmTabActive").removeClass("cmTab").addClass("cmTab");
		}
		});
		$('div[id$=_contain]','#aviation_contain').each(function(){
		id = $(this).attr("id");
		id_array = $(this).attr("id").split("_");
		idname = id_array[0];
		if(clickedname==idname){
		$(this).removeClass("none");
		} else {
		$(this).removeClass("none").addClass("none");
		}
		});
		if(clickedname=="airports") $.ajax({ url: "/auto/wui/geo/PrefDump/index.html?setprefs.0.key=AVIATION&setprefs.0.val=airports"});
		if(clickedname=="flights") $.ajax({ url: "/auto/wui/geo/PrefDump/index.html?setprefs.0.key=AVIATION&setprefs.0.val=flights"});
		if(clickedname=="pilot") $.ajax({ url: "/auto/wui/geo/PrefDump/index.html?setprefs.0.key=AVIATION&setprefs.0.val=pilot"});
		return false;
		});
	})(jQuery);
</script>
</div>
		</div>
