<script>
	jQuery(document).ready(function($) {
		var dots = $("div[class=newsDots] span");
		var lastPos = 0;
  var setupNewsSwipe = function() {
		window.newsSwipe = Swipe(document.getElementById('newsSlider'), {
		callback: function(index, elem) {
		if(dots) {
		$(dots[lastPos]).removeClass('active').addClass('inactive');
		$(dots[index]).removeClass('inactive').addClass('active');
		lastPos = index;
		}
		}
		});
		window.addEventListener('onorientationchange', window.newsSwipe.setup);
  $("div[class=newsDots] span").click( function() {
  var slideToPosition = $(this).data("position");
  window.newsSwipe.slide(slideToPosition);
  });
  }
  if($(window).width() < 640) {
            setupNewsSwipe();
        }

		/* Resize for scaling on desktop */
		$(window).resize(function() {
          if(!window.newsSwipe && $(window).width() < 640) {
            setupNewsSwipe();
          } else if (window.newsSwipe && $(window).width() >= 640) {
  window.newsSwipe.kill();
  $(".cmItem").attr('style', '');
  } else if (window.newsSwipe && $(window).width() < 640) {
             window.newsSwipe.setup(); 
          }
		});
	});
</script>
