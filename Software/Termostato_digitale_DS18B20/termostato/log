	<div id="iconspopcontainer"></div>
	<script type="text/javascript">
		(function($) {
		jQuery("a[class^='iconSwitch']").click(function() {
		icon = jQuery(this);
		popcontain = jQuery('<div class="iconSwitchPop none"></div>').insertAfter('#iconspopcontainer');
		jQuery(popcontain).load('/member/membersettings.html?responder=1&request=iconspopbox', function() {
		jQuery(popcontain).removeClass("none");
		jQuery(popcontain).position({ of: icon, my: "left top", at: "left top", offset: "0", collision: 'fit fit' });
		});
		return false;
		});
		$(".scrollAnchor").click(function(event){
		event.preventDefault();
		$('html,body').animate({scrollTop:$(this.hash).offset().top}, 500);
		});
		})(jQuery);
	</script>
