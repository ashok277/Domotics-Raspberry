(function(d){
	var js, id = 'facebook-jssdk', ref = d.getElementsByTagName('script')[0];
	if (d.getElementById(id)) {return;}
	js = d.createElement('script'); js.id = id; js.async = true;
	js.src = "//connect.facebook.net/en_US/all.js";
	ref.parentNode.insertBefore(js, ref);
}(document));
jQuery(document).ready(function($) {
	var shareCategory = 'CityPage';
	var sharePage = 'IntlCity';
  if (wui.ToolPanel) {
  var shareToolPanel = new wui.ToolPanel ({
  button: "#shareButton",
  panel: "#sharePanel",
  group: "share",
  selectedClass: "linkActive",
  position: {
  my: "right top",
  at: "right bottom"
  },
  onShow: function() {
  setMiniURL();
		$("body").on("click", function(e) {
		if(!$(e.target).is("#sharePanel") && $(e.target).parents('#share').length == 0 && $(e.target).parents('#sharePanel').length == 0){
  shareToolPanel.hide();
		$(this).off();
		}
		});
  }
  });
