		<script type="text/javascript">
		function SubmitFastForecastSearch(){
		var url=new String();
		var search = document.getElementById('wuSearch').value;
		url='/cgi-bin/findweather/getForecast?query='+search;
		window.location = url;
		return true;
		}
		</script>
