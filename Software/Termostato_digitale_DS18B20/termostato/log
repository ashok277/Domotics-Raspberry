	function setMiniURL() {
		locationurl="http://www.wunderground.com/cgi-bin/findweather/getForecast?query=cesena";
		var miniurl = $.ajax({
		url: "/miniurl/api.php?url="+locationurl,
		async: false
		}).responseText;
		$('#urlInput').val(miniurl);
	}
	$("#shortLinkRadio").click(function() {
		setMiniURL();
	});
	$("#fullLinkRadio").click(function() {
		locationurl="http://www.wunderground.com/cgi-bin/findweather/getForecast?query=cesena";
		$('#urlInput').val(locationurl);
	});
	$("#socialFacebook").click(function() {
		if (shareCategory==="CityPage") {
		facebook_share_forecast('http://www.wunderground.com/.html', 'http://icons-ak.wxug.com/i/c/k/partlycloudy.gif', 'Partly Cloudy', '21 &deg;C', '23 &deg;C', '14 &deg;C', 'Mostly Cloudy', '22 &deg;C', '11 &deg;C', 'Rain', '16 &deg;C', '7 &deg;C', 'Cesena, ');
		}
		else if (sharePage==="ViewWxImage") {
		facebook_share_wunderphoto('', 'http://www.wunderground.com/cgi-bin/findweather/getForecast?query=cesena', '', 'http://icons-ak.wunderground.com', '', '');
		}
		else if (shareCategory==="Blog" && sharePage==="Article" || shareCategory==="Blog" && sharePage==="Comment") {
		facebook_share_blog('http://www.wunderground.com/blog//comment.html', '', '');
		}
		else if (sharePage==="WorldView") {
		facebook_share_worldview('World View: Photos from Around the World', 'http://www.wunderground.com/worldview', 'http://icons.wunderground.com','');
		}
		else {
		facebook_share_page('Cesena, Italy Forecast', 'http://www.wunderground.com/cgi-bin/findweather/getForecast?query=cesena', '', 'http://icons-ak.wxug.com/i/wu/wuLogoUrl.png');
		}
		$("#sharePanel").hide();
		$("#shareButton").removeClass("linkActive");
		return false;
	});
	$("#socialTwitter").click(function() {
		if (sharePage==="WorldView") {
		var string = 'World View: Photos from around the world. Collection # ';
		var twitterString = encodeURIComponent(string);
		var via = '&via=wunderground&';
		locationurl="http://www.wunderground.com/worldview/?collection=";
		}
		else if (shareCategory==="CityPage") {
		var via = '&via=wunderground&';
		var string = 'Cesena, Italy - Now: Partly Cloudy \(21C\) Tomorrow: Rain \(16C/7C\)';locationurl='http://www.wunderground.com/.html';
		var twitterString = encodeURIComponent(string);
		}
		else if (shareCategory==="Blog" && sharePage==="Article" || shareCategory==="Blog" && sharePage==="Comment") {
		var string = '';
		var twitterString = encodeURIComponent(string);
		locationurl="http://www.wunderground.com/cgi-bin/findweather/getForecast?query=cesena";
		console.log('');
		var via = '&';
		console.log(via);
		}
		else {
		var string = 'Cesena, Italy Forecast';
		var twitterString = encodeURIComponent(string);
		var via = '&via=wunderground&';
		locationurl="http://www.wunderground.com/cgi-bin/findweather/getForecast?query=cesena";
		}
		var twitterString = encodeURIComponent(string);
		var miniurl = $.ajax({
		url: "/miniurl/api.php?url="+locationurl,
		async: false
		}).responseText;
		if (miniurl && miniurl.length > 0) {
		pageurl="http://twitter.com/share?url=" + encodeURIComponent(miniurl) + via + "related=wunderground&text=" + twitterString;
		}
		else {
		pageurl="http://twitter.com/share?url=" + locationurl + via + "related=wunderground&text=" + twitterString;
		}
		window.open(pageurl,"twitterwindow","menubar=1,resizable=0,width=550,height=368");
		$("#sharePanel").hide();
		$("#shareButton").removeClass("linkActive");
		return false;
	});
	$("#socialGoogle").click(function() {
		locationurl="http://www.wunderground.com/cgi-bin/findweather/getForecast?query=cesena";
		pageurl="https://plus.google.com/share?url=" + locationurl;
		window.open(pageurl, '', 'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=600,width=600');
		$("#sharePanel").hide();
		$("#shareButton").removeClass("linkActive");
		return false;
	});
	$("#socialReddit").click(function() {
		locationurl="http://www.wunderground.com/cgi-bin/findweather/getForecast?query=cesena";
		pageurl = 'http://www.reddit.com/submit?url=' + locationurl;
		window.open(pageurl, "redditwindow","menubar=1,resizable=0,width=750,height=500");
		$("#sharePanel").hide();
		$("#shareButton").removeClass("linkActive");
	});
	$("#socialMail").click(function() {
		$("#sharePanel").hide();
		$("#shareButton").removeClass("linkActive");
	});
});
</script>
