			//unchangedColor: null
		});
	})();

	(function(wui, $){
		reportpop_open = function(){
			container = $("#reportstationbox");
			pwsreport = $("#pwsflag");
			container.position({ of: pwsreport, my: "left top", at: "left top", offset: "0", collision: 'fit fit' });
			container.removeClass("none");
		}
		reportpophide = function(){
			container = $("#reportstationbox");
			container.addClass("none");
		}
		new wui.tooltip({
			button: $('#pwsflag'),
			content: 'Click the "report" link only if you think this station is reporting bad data.<br /><br />After reporting a station you can select a different station using the "Station Select" button.',
		hoverClass: "hover",
		position: {
		collision: "fit none"
		}
		});
		flagstation = function(stationid, zip, magic, wmo, englishtemp, pressure, badtemp, badpress, badwind, badfct){
		$("#flagstationbutton").attr("disabled","disabled");
		//console.log("Got stationid:" + stationid + " zip:" + zip + " magic:" + magic + " wmo:" + wmo + " temp_e:" + englishtemp + " pressure:" + pressure + " badtemp:" + badtemp + " badpress:" + badpress + " badwind:" + badwind + " badfct:" + badfct);
		if(badtemp==false && badpress==false && badwind==false && badfct==false){
		$("#report_errorbox").text('You must select an error type.');
		$("#report_errorbox").removeClass('none');
		$("#flagstationbutton").attr("disabled","");
		} else{
		$.ajax({
		url: "/wuflag/flag.html?type=wxstation&stationid="+stationid+"&zip="+zip+"&magic="+magic+"&wmo="+wmo+"&temp_e="+englishtemp+"&press="+pressure+"&badtemp="+badtemp+"&badpress="+badpress+"&badwind="+badwind+"&badfct="+badfct,
		error: function(data, textStatus){
		reportpophide();
		$("#flagstationbutton").attr("disabled","");
		},
		success: function(data, textStatus) {
		reportpophide();
		$("#flagstationbutton").attr("disabled","");
		}
		});
		$("#report_errorbox").text('');
		$("#report_errorbox").addClass('none');
		}
		}
	})(wui,jQuery);
</script>
		<script type="text/javascript">
