<script type="text/javascript">
	(function($){
		$('#discussion_toggle, #cont_discussion, #talkingHead').click(function(){
		if($('#full_discussion').hasClass('none')){
		$('#discussion_contain').slideToggle('fast',function(){
		$('#full_discussion').removeClass('none');
		$('#short_discussion').addClass('none');
		$('#cont_discussion').addClass('none');
		$('#discussion_toggle').removeClass('cmToggleDown');
		$('#discussion_toggle').addClass('cmToggleUp');
		$('#discussion_contain').slideToggle('fast');
		});
		} else {
		$('#discussion_contain').slideToggle('fast',function(){
		$('#full_discussion').addClass('none');
		$('#short_discussion').removeClass('none');
		$('#cont_discussion').removeClass('none');
		$('#discussion_toggle').removeClass('cmToggleUp');
		$('#discussion_toggle').addClass('cmToggleDown');
		$('#discussion_contain').slideToggle('fast');
		});
		}
		return false;
		});
	})(jQuery);
</script>
