<script type="text/javascript">
	(function($) {
		$("#socialTwitter").click(function() {
		locationurl="http%3A%2F%2Fwww.wunderground.com%2Fcgi-bin%2Ffindweather%2FgetForecast%3Fquery%3Dzmw%3A00000.5.16148";
		var miniurl = $.ajax({
		url: "/miniurl/api.php?url="+locationurl,
		async: false
		}).responseText;
		//console.log(miniurl);
		if(miniurl && miniurl.length>0)
		pageurl="http://twitter.com/share?url=" + encodeURIComponent(miniurl) + "&via=wunderground&related=wunderground&text=";
		else
		pageurl="http://twitter.com/share?url=" + locationurl + "&via=wunderground&related=wunderground&text=";
		window.open(pageurl,"twitterwindow","menubar=1,resizable=0,width=550,height=370");
		return false;
		});
	})(jQuery);
	jQuery("div#socialWeather > a[id^='social']").each(function(index) {
		jQuery(this).hover(
		function () {
		jQuery(this).parent().css('background-position','0px -' +(26*(index+1))+ 'px');
		},
		function () {
		jQuery(this).parent().css('background-position','0px 0px');
		}
		);
	});
	jQuery("a[id='rfToggle']",'#rfSwitch').click(function() {
		if(jQuery(this).attr('class')=='rfToggleOffBlue'){
		jQuery(this).attr('href','/cgi-bin/findweather/getForecast?setpref=PWSRT&value=0&referer=%2fcgi%2dbin%2ffindweather%2fgetForecast%3fquery%3dcesena');
		jQuery(this).attr('class', 'rfToggleOnBlue');
		wui.rapidfire.start();
		jQuery.ajax({ url: "/?setprefs.0.key=PWSRT&setprefs.0.val=1"});
		_gaq.push(['_trackEvent', 'RapidFire', 'Turned On']);
		}
		else{
		jQuery(this).attr('href','/cgi-bin/findweather/getForecast?setpref=PWSRT&value=1&referer=%2fcgi%2dbin%2ffindweather%2fgetForecast%3fquery%3dcesena');
		jQuery(this).attr('class', 'rfToggleOffBlue');
		wui.rapidfire.stop();
		jQuery.ajax({ url: "/?setprefs.0.key=PWSRT&setprefs.0.val=0"});
		_gaq.push(['_trackEvent', 'RapidFire', 'Turned Off']);
		}
		return false;
	});
	StickerPopUp = { "selected" : "wu_bluestripes" };
	jQuery(document).ready(function(){
		jQuery("div#sticker_preview").attr("initialized", "false");
	})
	StickerPopUp.swapPreview = function(style){
		jQuery(document.getElementById(this.selected)).addClass("sticker_style");
		jQuery(document.getElementById(this.selected)).removeClass("sticker_style_selected");
		document.getElementById("final_img").src = "http://weathersticker.wunderground.com/cgi-bin/banner/ban/wxBanner?bannertype="+style+"&airportcode=LIPC&ForcedCity=Cesena&ForcedState=";
		document.getElementById("final_html").value = '<a href="http://www.wunderground.com/cgi-bin/findweather/getForecast?query=zmw:00000.5.16148&bannertypeclick='+style+'"><img src="http://weathersticker.wunderground.com/cgi-bin/banner/ban/wxBanner?bannertype='+style+'&airportcode=LIPC&ForcedCity=Cesena&ForcedState=" alt="Click for Cesena, Italy Forecast" height="90" width="160" /></a>';
		//document.getElementById("final_html").value = '<a href="http://www.wunderground.com/global\x2Fstations\x2F16148.html?bannertypeclick='+style+'"><img src="http://weathersticker.wunderground.com/cgi-bin/banner/ban/wxBanner?bannertype='+style+'&airportcode=LIPC&ForcedCity=Cesena&ForcedState=" alt="Click for Cesena, Italy Forecast" height="90" width="160" /></a>';
		jQuery(document.getElementById(style)).removeClass("sticker_style");
		jQuery(document.getElementById(style)).addClass("sticker_style_selected");
		this.selected = style;
	}
	StickerPopUp.togglePreview = function(active){
		var preview = document.getElementById("sticker_preview");
		if(jQuery("div#sticker_preview").attr("initialized") == "false"){
		jQuery("div#sticker_preview").attr("initialized", "true");
		document.getElementById("final_img").src = "http://weathersticker.wunderground.com/cgi-bin/banner/ban/wxBanner?bannertype=wu_bluestripes&airportcode=LIPC&ForcedCity=Cesena&ForcedState=";
		jQuery("div#sticker_preview").position({ of:document.getElementById("locationBarBottom"), my: "left top", at: "center", offset: "0", collision: "fit fit"});
		}
		if(active){
		jQuery(preview).removeClass("none");
		}else{
		jQuery(preview).addClass("none");
		}
	};
	StickerPopUp.forceHide = function(){
		var preview = document.getElementById("sticker_preview");
		jQuery(preview).addClass("none");
	};
	jQuery("#expFctToggle").click(function() {
		if(jQuery(this).attr('class')=='rfToggleNWS'){
		jQuery(this).attr('href','/cgi-bin/findweather/getForecast?setpref=EXPFCT&value=0&referer='+referer);
		jQuery(this).attr('class', 'rfToggleBest');
		jQuery.ajax({
		url: "/?setprefs.0.key=EXPFCT&setprefs.0.val=1",
		success: function(data, textStatus) {
		if (conditionsurl) {
		jQuery('#curConds').fadeOut('slow');
		jQuery.ajax({
		url: conditionsurl,
		dataType: "html",
		data: {
		"responder": "1",
		"request": "conditions"
		},
		success: function(data, textStatus) {
		jQuery('#curConds').html(data);
		jQuery('#curConds').fadeIn('slow',function() {
		jQuery('#curConds').children().eq(0).unwrap();
		});
		jQuery('#layerForecast').fadeOut('slow');
		jQuery.ajax({
		url: conditionsurl,
		dataType: "html",
		data: {
		"responder": "1",
		"request": "forecast"
		},
		success: function(data, textStatus) {
		jQuery('#layerForecast').html(data);
		jQuery('#layerForecast').fadeIn('slow', function() {
		});
		}
		});
		}
		});
		}
		}
		});
		_gaq.push(['_trackEvent', 'Forecast Toggle', 'Use BestForecast']);
		} else {
		jQuery(this).attr('href','/cgi-bin/findweather/getForecast?setpref=EXPFCT&value=1&referer='+referer);
		jQuery(this).attr('class', 'rfToggleNWS');
		jQuery.ajax({
		url: "/?setprefs.0.key=EXPFCT&setprefs.0.val=0",
		success: function(data, textStatus) {
		if (conditionsurl) {
		jQuery('#curConds').fadeOut('slow');
		jQuery.ajax({
		url: conditionsurl,
		dataType: "html",
		data: {
		"responder": "1",
		"request": "conditions"
		},
		success: function(data, textStatus) {
		jQuery('#curConds').html(data);
		jQuery('#curConds').fadeIn('slow',function() {
		jQuery('#curConds').children().eq(0).unwrap();
		});
		jQuery('#layerForecast').fadeOut('slow');
		jQuery.ajax({
		url: conditionsurl,
		dataType: "html",
		data: {
		"responder": "1",
		"request": "forecast"
		},
		success: function(data, textStatus) {
		jQuery('#layerForecast').html(data);
		jQuery('#layerForecast').fadeIn('slow', function() {
		});
		}
		});
		}
		});
		}
		}
		});
		_gaq.push(['_trackEvent', 'Forecast Toggle', 'Use NWS']);
		}
		return false;
	});
</script>
