		<script type="text/javascript">
		(function(id,$){
		//AMW 20130813
		//This was moved to site-wide utils JS section
		//Function masks the number of shards for this case
		//Leftover from when the function was first written here
		var urlAliasHash = function(urlToHash){
		return wui.tools.domainShardSuffix(urlToHash, 8);
		}
		var wunderimages = [ { "image": "http://icons-ak"+urlAliasHash("/data/wximagenew/s/sassyvintage/312-thumb250.jpg")+".wxug.com/data/wximagenew/s/sassyvintage/312-thumb250.jpg", "height": "183", "title":"Busy sunset Nov 4", "loaded": true }, { "image": "http://icons-ak"+urlAliasHash("/data/wximagenew/n/Nikongranny/8528-thumb250.jpg")+".wxug.com/data/wximagenew/n/Nikongranny/8528-thumb250.jpg", "height": "166", "title":"So Much Color 3", "loaded": true }, { "image": "http://icons-ak"+urlAliasHash("/data/wximagenew/b/BettyFrance/1528-thumb250.jpg")+".wxug.com/data/wximagenew/b/BettyFrance/1528-thumb250.jpg", "height": "167", "title":"Lake Kegonsa Sun Pillar", "loaded": false }, { "image": "http://icons-ak"+urlAliasHash("/data/wximagenew/h/HOLLY/333-thumb250.jpg")+".wxug.com/data/wximagenew/h/HOLLY/333-thumb250.jpg", "height": "166", "title":"This Mornings Sunrise", "loaded": false }, { "image": "http://icons-ak"+urlAliasHash("/data/wximagenew/n/Necdet/1691-thumb250.jpg")+".wxug.com/data/wximagenew/n/Necdet/1691-thumb250.jpg", "height": "166", "title":"Sapanca Lake 04", "loaded": false }, { "image": "http://icons-ak"+urlAliasHash("/data/wximagenew/l/llpj04/8561-thumb250.jpg")+".wxug.com/data/wximagenew/l/llpj04/8561-thumb250.jpg", "height": "167", "title":"Flock Of Gulls", "loaded": false }, { "image": "http://icons-ak"+urlAliasHash("/data/wximagenew/r/RIWXPhoto/15668-thumb250.jpg")+".wxug.com/data/wximagenew/r/RIWXPhoto/15668-thumb250.jpg", "height": "188", "title":"Rhode Island Autumn Colors@ Newport # 3", "loaded": false }, { "image": "http://icons-ak"+urlAliasHash("/data/wximagenew/r/randel52/7462-thumb250.jpg")+".wxug.com/data/wximagenew/r/randel52/7462-thumb250.jpg", "height": "188", "title":"Just a baby", "loaded": false }, { "image": "http://icons-ak"+urlAliasHash("/data/wximagenew/r/Railheel/1266-thumb250.jpg")+".wxug.com/data/wximagenew/r/Railheel/1266-thumb250.jpg", "height": "167", "title":"Upper Falls 1", "loaded": false }, { "image": "http://icons-ak"+urlAliasHash("/data/wximagenew/t/teach50/9403-thumb250.jpg")+".wxug.com/data/wximagenew/t/teach50/9403-thumb250.jpg", "height": "167", "title":"Evening Is Upon Us", "loaded": false } ];
		var prev = $("a[class=prev]",id);
		var next = $("a[class=next]",id);
		var photoby = $("a[id=photoByName]","#wunderphotos_contain");
		var photos = $("div[class=elt]",id);
		var dots = $("div[class=dots] span",id);
		var pos = 0;
		var pos_prev = 0;
		var max = photos.length - 1;
		$.loadwunderimage = function(index){
		if(wunderimages[index]["loaded"])
		return;
		var img = new Image();
		var imgclass="fitWidth";
		if(wunderimages[index]["height"]<=185)
						imgclass="fitHeight";
					$(img).load(function () {
						$('#thumb_'+index).append(this);
						wunderimages[index]["loaded"]=true;
					}).error(function () {
						wunderimages[index]["loaded"]=false;
						//console.log("Image ['"+images[index]["image"]+"'] Failed to Load");
					}).attr({'src': wunderimages[index]["image"], 'class':imgclass, 'alt':wunderimages[index]["title"] });
				};

				var scrollBy = function(i) {
					scrollLoc(pos + i);
				};

				var scrollLoc = function(i) {
					pos_prev = pos;
					if (i < 0)
						pos = 0;
					else if (i > max)
		pos = max;
		else
		pos = i;
		$.loadwunderimage(pos);
		var elt = photos.eq(pos);
		$(photoby).html(elt.attr("name"));
		$(photoby).attr("href","/wximage/"+elt.attr("name")+"");
		if (dots) {
		$(dots[pos_prev]).removeClass("active").addClass("inactive");
		$(dots[pos]).removeClass("inactive").addClass("active");
		}
		if(pos < max)
						$.loadwunderimage(pos + 1);
				};

				$(prev).click(function(){
					window.photoSwipe.prev();
				});
				$(next).click(function(){
					window.photoSwipe.next();
                  });
                $("div[class=dots] span").click( function() {                                                                               
                    var slideToPosition = $(this).data("position");                                                                             
                    window.photoSwipe.slide(slideToPosition);                                                                                    
                });   

			$( function() {
				window.photoSwipe = Swipe(document.getElementById('wunderPhotoSlider'), {
					callback: function(index, elem) {
						scrollLoc(parseInt(index));
					}
				});
			});

			})("#wunderphotos_contain",jQuery);	
		</script>
	</div>
	<div class="cmFooter brBot5"><a title="WunderPhotos - Click Here for Latest Weather Photos" href="/wximage/">Visit WunderPhotos</a></div>
</div>
